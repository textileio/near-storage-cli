{
  "name": "@textile/near-storage-cli",
  "version": "0.0.0",
  "description": "Command-line utilities for Textile's Broker-based data storage system on the Near blockchain.",
  "main": "./dist/index.js",
  "bin": {
    "bridge": "./bin/bridge"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/*.js?(.map)",
    "dist/*.d.ts",
    "src/*.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint --fix"
  },
  "keywords": [
    "near",
    "textile",
    "cli",
    "storage",
    "filecoin"
  ],
  "author": "Textile Inc <contact@textile.io>",
  "license": "(MIT AND Apache-2.0)",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-jest-dom": "^3.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@textile/near-storage": "^6.0.2",
    "@types/node-fetch": "^2.5.10",
    "chalk": "^2.4.2",
    "dotenv": "^10.0.0",
    "form-data": "^3.0.1",
    "form-data-encoder": "^1.0.1",
    "formdata-node": "^3.5.4",
    "near-api-js": "^0.41.0",
    "node-fetch": "^2.6.1",
    "yargs": "^15.4.1"
  }
}